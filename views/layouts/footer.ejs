        <footer>
            <main>
                <section class="background-dark">
                    <%- include('../partials/footer') %>
                </section>
            </main>
        </footer>
    </div>
    <script>


        const hamBurgBtn = document.getElementById('hbMenu');
        const sideNav = document.getElementById('sideNavWrapper');
        const closeNavBtn = document.getElementById('closeBtn');
        
        
        hamBurgBtn.addEventListener('click', () => {
            sideNav.style.display = "flex";
            closeNavBtn.style.display = 'flex';
        });
        closeNavBtn.addEventListener('click', () => {
            sideNav.style.display = 'none';
            closeNavBtn.style.display = 'none';

        });

        
        const modal = document.getElementById('modal');
        const closeBtn = document.getElementById('modal-closeBtn');
        let modalImg = document.getElementById('img01');

        const modalBackground = document.getElementById('modal-background');
        const images = document.getElementsByClassName('project-thumb-img');

        for (var i = 0; i < images.length; i++) {
            var img = images[i];
            img.onclick = function() {
                modal.style.display = 'flex';
                modalBackground.style.display = 'flex';
                setTimeout(() => {
                    modalBackground.style.opacity = '0.8';

                }, 500);
                setTimeout(() => {
                    modal.style.opacity = "1";

                }, 1000);
                modalImg.src = this.src;
            }
        }


       
        closeBtn.onclick = function() {
            modal.style.opacity = '0';
            modalBackground.style.opacity = '0';
            setTimeout(() => {
                modal.style.display = 'none';
            }, 500);
            setTimeout(() => {
                modalBackground.style.display = 'none';
            }, 1000);
            
        }
        window.onclick = function(event) {
            if (event.target == modal) {
                modal.style.opacity = '0';
                modalBackground.style.opacity = '0';
            setTimeout(() => {
                modal.style.display = 'none';
            }, 500);
            setTimeout(() => {
                modalBackground.style.display = 'none';
            }, 1000);
            }
        }




        // Contact Form
        const contactFrom = document.getElementById('contactForm');

        contactFrom.addEventListener('submit', (e) => {
            e.preventDefault();
            console.log('submit clicked')
        })

</script>
    
</body>
</html>