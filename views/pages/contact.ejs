<%- include('../layouts/header') %>
<main>
    <section class="background-dark">
        <div id="projects-header" class="grid-gap-colum-10">
            <h3>Contact Me</h3>
        </div>
    </section>
    <section class="background-medium">
        <div id="contact-section" >
            <form action="https://formsubmit.co/alex.coldicott@outlook.com" method="POST" id="contactForm">
                <div>
                    <input type="text" name="name" placeholder="name">
                </div>
                <div>
                    <input type="email" name="email" placeholder="email">
                </div>
                <div>
                    <input type="number" name="number" placeholder="number">
                </div>
                <div>
                    <textarea name="confirmationText"></textarea>
                </div>
                <div>
                    <button class="btn" type="submit">Send</button>

                </div>
            </form>
        </div>
    </section>
</main>
<%- include('../layouts/footer') %>
